sers/isuriidda/Documents/user-react-dashboard/src/components/reusable/discountCard.jsx",[],[],"/Users/isuriidda/Documents/user-react-dashboard/src/components/reusable/DiscountCard.jsx",["170","171"],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/index.js",[],[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/App.js",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/Layout/Layout.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/routes/Router.js",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/TopNav/TopNav.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/Sidebar/Sidebar.jsx",["172","173","174"],[],"import React,{useEffect,useState} from \"react\";\nimport { NavLink } from \"react-router-dom\";\nimport navLinks from '../../assets/dummy data/navLinks'\nimport \"./Sidebar.css\";\nimport { Link } from \"react-router-dom\";\nimport profileImg from '../../assets/images/images-2.jpeg';\nimport logo from '../../assets/images/logo1.jpeg';\nimport MyProfile from \"../../Pages/MyProfile\";\n\n\n  \n\n\n\nconst Sidebar = ({}) => {\n\n    const [fnames, SetFnames]=useState([])\n   \n    useEffect(()=>{\n       \n        fetch(\"http://localhost:8080/user/getAll\")\n        .then(res=>res.json())\n        .then((result)=>{ \n        SetFnames(result);\n      }\n      )\n    },[])\n\n    return  (\n    <div className=\"sidebar\">\n\n\n            <div className=\"logo\">\n               \n               <img src={logo} alt=\"\" className=\"img1\"/>\n              \n            </div>\n                   \n            \n            <div className=\"profile\">            {/*profile*/}\n                    <span>\n                        <Link to ='/myprofile'><img src={profileImg} alt=\"\"/></Link>\n                    </span>\n                          <h3>\n                          Hello, Ann\n                         \n                          </h3>\n            </div>\n            \n\n        <div className =\"sidebar__content\" >        {/*sidebar content*/}\n            <div className=\"menu\">\n                <ul className=\"nav__list\">\n                    {navLinks.map((item,index) => (\n                    <li className=\"nav__item\" key={index}>\n                        <NavLink \n                            to = {item.path}\n                            className={(navClass) =>\n                                navClass.isActive ? \"nav__active nav__link\" : \"nav__link\"\n                            }\n                            >\n                                <i className={item.icon}></i>\n                            \n                            {item.Display}\n                            \n                        </NavLink>\n                    </li>\n                    ))}\n                </ul>\n            </div>\n\n            <div className=\"sidebar__bottom\">            {/*sidebar bottom*/}\n                <span>\n                    <i className=\"ri-logout-circle-r-line\"></i> Logout\n                </span>\n            </div>\n        </div>\n    </div>\n    );\n};\nexport default Sidebar;","/Users/isuriidda/Documents/codes/user-react-dashboard/src/Pages/MyDiscounts.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/Pages/Dashboard.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/Pages/MyProfile.jsx",["175","176","177","178","179","180","181","182"],[],"import React, { useState } from \"react\";\nimport \"../styles/myprofile.css\";\nimport Sidebar from \"../components/Sidebar/Sidebar\";\n\n\n \n\n\nconst MyProfile = () => {\n  \n  const[fname, setFname]=useState('')\n  const[lname, setLname]=useState('')\n  const[nic, setNic]=useState('')\n  const[email, setEmail]=useState('')\n  const[mnumber, setMnumber]=useState('')\n  const[address, setAddress]=useState('')\n  const[city, setCity]=useState('')\n  const[error,setError]=useState(false)\n  const[message, setMessage]=useState(null)\n\n  function isValidEmail(email){\n    return /\\S+@\\S+\\.\\S+/.test(email);\n  }\n  \n  const handleChange = e => {\n    if (!isValidEmail(e.target.value)) {\n      setMessage('Email is invalid');\n    } else {\n      setMessage(null);\n    }\n    setEmail(e.target.value);\n\n   \n  };\n\n\n  const handleClick=(e)=>{\n\n    e.preventDefault()\n    const user={fname,lname,nic,email,mnumber,address,city}\n\n    if(fname.length==0 || lname.length==0 || nic.length==0 || email.length==0 || mnumber.length==0 || address.length==0 || city.length==0)\n        setError(true)\n    \n        if(fname && lname && nic && email && mnumber && address && city)\n        {\n          console.log(user)\n          fetch(\"http://localhost:8080/user/add\" , {\n            method:\"POST\",\n            headers:{\"Content-Type\":\"application/json\"},\n            body:JSON.stringify(user)\n          }).then(()=>{\n            console.log(\"New User Added\")\n            alert('Your information was saved' )\n          })\n        }   \n        \n        \n    \n  }\n  \n    \n  \n  \n \n    return(\n      \n        <div className=\"myprofile\">\n\n         \n            <h2 className=\"myprofile__title\">My Profile</h2>\n\n            <div className=\"myprofile__wrapper\">\n                \n               \n        <div className=\"details__form\">        {/*form to collect data*/}\n          <h2 className=\"profile__title\">Your Details</h2>\n          <p className=\"profile__desc\">\n           \n          </p>\n          <form onSubmit={handleClick}>\n            <div className=\"form__group\">\n\n            <div>\n                <label>First Name</label>\n                <input type=\"text\" placeholder=\"\"\n                value={fname}\n                onChange={(e)=>setFname(e.target.value)}/>  \n                \n                {error&&fname.length<=0?\n              <label class=\"errorNote\">First Name can't be empty</label>:\"\"}\n\n            </div>\n\n              \n              <div>\n                <label>Last Name</label>\n                <input type=\"text\" placeholder=\"\" \n                value={lname}\n                onChange={(e)=>setLname(e.target.value)}/>\n                \n                {error&&lname.length<=0?\n              <label class=\"errorNote\">Last Name can't be empty</label>:\"\"}\n              </div>\n              </div>\n            \n\n              <div className=\"form__group\">\n              <div>\n                <label>NIC</label>\n                <input type=\"text\" placeholder=\"\"\n                value={nic}\n                onChange={(e)=>setNic(e.target.value)} />\n                {error&&nic.length<=0?\n              <label class=\"errorNote\">NIC can't be empty</label>:\"\"}\n              </div>\n\n              \n\n\n              <div>\n                <label>Email</label>\n                <input type=\"email\" placeholder=\"\" \n                value={email}\n                onChange={handleChange} /> \n                {message && <h4 style={{color: 'red'}}>{message}</h4>}\n    \n                {error&&email.length<=0?\n              <label class=\"errorNote\">Email can't be empty</label>:\"\"}\n              </div>\n\n              \n            </div>\n\n\n            <div className=\"form__group\">\n            <div>\n                <label>Mobile Number</label>\n                <input type=\"text\" placeholder=\"\" \n                value={mnumber}\n                onChange={(e)=>setMnumber(e.target.value)}/>\n                {error&&mnumber.length<=0?\n             <label class=\"errorNote\">Mobile Number can't be empty</label>:\"\"}\n             </div>\n\n             \n\n\n              <div>\n                <label>Address</label>\n                <input type=\"text\" placeholder=\"\" \n                value={address}\n                onChange={(e)=>setAddress(e.target.value)}/>\n                {error&&address.length<=0?\n              <label class=\"errorNote\">Address can't be empty</label>:\"\"}\n\n              </div> \n\n              \n            </div>\n\n            <div className=\"form__group\">\n              <div>\n                <label>City</label>\n                <input type=\"\" placeholder=\"\" \n                value={city}\n                onChange={(e)=>setCity(e.target.value)}/>\n                {error&&city.length<=0?\n              <label class=\"errorNote\">City can't be empty</label>:\"\"}\n              </div>\n\n              \n\n            </div>\n\n            <div className=\"form__group\">\n\n            \n              \n              <div>\n                \n                <label>Your Photo</label>\n                <p className=\"profile-img__desc\">\n                  This will be displayed in your profile\n                </p>\n                <input type=\"file\" placeholder=\"choose file\"/>   {/*uploading the profile image*/}\n              </div>\n\n              \n                <button onClick={handleClick} className=\"update__btn\">Save </button>\n                \n             \n            </div>\n          </form>\n        </div>\n       </div>\n      </div>\n    );\n};\n\nexport default MyProfile;","/Users/isuriidda/Documents/codes/user-react-dashboard/src/Pages/History.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/assets/dummy data/navLinks.js",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/reusable/DiscountCard.jsx",["183","184"],[],"import React from \"react\";\nimport '../../styles/mydiscounts.css';\n\n\n//Destructure the title, description and icon from the props object\nconst DiscountCard = (props) => {\n\n    const {title, description, icon} = props.item\n    return(\n        <div className=\"discount__card\">\n                        <div className=\"discard__content\">\n                            <h4>{title}</h4>\n                            <span>{description}</span>\n                            </div>\n                            <span className=\"discard__icon\">\n                               <i className={icon}></i>\n                            </span>\n                            \n                            <div className=\"discard__actions\">\n                            <button className=\"discard__btn\">Get My Coupon Code</button>\n                            <a href=\"#\"></a>     {/*This link doesn't have any text or content*/}\n        </div>\n                    </div>\n    );\n};\n\nexport default DiscountCard;\n\n","/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/reusable/SingleCard.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/Table/Table2.jsx",[],[],"/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/Table/Table.jsx",["185","186","187","188","189","190","191","192","193","194","195","196","197"],[],"// Importing React and necessary Material-UI components\nimport React,{useEffect,useState} from 'react';\nimport Table from '@mui/material/Table';\nimport TableBody from '@mui/material/TableBody';\nimport TableCell from '@mui/material/TableCell';\nimport TableContainer from '@mui/material/TableContainer';\nimport TableHead from '@mui/material/TableHead';\nimport TableRow from '@mui/material/TableRow';\nimport Paper from '@mui/material/Paper';\nimport \"./Table.css\";\nimport Dashboard from '../../Pages/Dashboard';\n\n\n\n\n\n\n\n// Function to set style based on status\nconst makeStyle=(status)=>{\n  if(status === 'Approved')\n  {\n     return{\n      background: 'rgb(145 254 159 / 47%)',\n      color: 'green'\n     }\n  }\n  else if(status === 'Pending')\n  {\n    return{\n      background: '#ffadad8f',\n      color: 'red',\n    }\n  }\n  else{\n    return{\n      background: '#59bfff',\n      color: 'white',\n    }\n  }\n}\n\n// React component for displaying a basic table with the rows of data\nexport default function BasicTable() {  \n  \n  \n\n  \n  \n\n  \n  const[description, SetDescription]=useState('')\n  const[service_name, SetService_name]=useState('')\n  const[price, SetPrice]=useState('')\n  const[pickup_date, SetPickup_date]=useState('')\n  const[delivery_date, SetDelivery_date]=useState('')\n  const[status, SetStatus]=useState('')\n  const[order_statuses, SetOrder_statuses]=useState([])\n\n  useEffect(()=>{\n    fetch(\"http://localhost:8081/order_status/getAll\")\n    .then(res=>res.json())\n    .then((result)=>{ \n    SetOrder_statuses(result);\n  }\n  )\n},[])\n\n  return (\n    \n     // Styling the container for the table\n    <div className=\"Table\"> \n    \n          \n        <h2>Order Status</h2>\n   \n    <TableContainer component={Paper}\n    style={{boxShadow: '0px 13px 20px 0px #80808029'}}\n    >\n      <Table sx={{ minWidth: 650 }} aria-label=\"simple table\">\n        <TableHead>\n          <TableRow>\n            <TableCell>Ord.Id</TableCell>\n            <TableCell align=\"left\">Description</TableCell>\n            <TableCell align=\"left\">Service Name</TableCell>\n            <TableCell align=\"left\">Price (Rs.)</TableCell>\n            <TableCell align=\"left\">Pickup Date</TableCell>\n            <TableCell align=\"left\">Delivery Date</TableCell>\n            <TableCell align=\"left\">Status</TableCell>\n          </TableRow>\n        </TableHead>\n        <TableBody>\n        {order_statuses.map(order_status=>(                 //loop through the 'rows' array and render each object as a table row\n            <TableRow\n              key={order_status.name}\n              sx={{ '&:last-child td, &:last-child th': { border: 0 } }}\n            >\n              <TableCell component=\"th\" scope=\"row\">\n                {order_status.id}\n              </TableCell>\n              <TableCell align=\"left\">{order_status.description}</TableCell>\n              <TableCell align=\"left\">{order_status.service_name}</TableCell>\n              <TableCell align=\"left\">{order_status.price}</TableCell>\n              <TableCell align=\"left\">{order_status.pickup_date}</TableCell>\n              <TableCell align=\"left\">{order_status.deliver_date}</TableCell>\n              <TableCell align=\"left\">\n              <span className=\"status\" style={makeStyle(order_status.status)}> {order_status.status}</span>\n                \n                  \n                \n                {/*styles based on the status of the order*/}\n              </TableCell>\n              \n            </TableRow>\n          ))}\n        </TableBody>\n      </Table>\n    </TableContainer>\n\n   \n    \n    </div>\n\n  \n\n  );\n  \n  \n  \n}\n","/Users/isuriidda/Documents/codes/user-react-dashboard/src/components/reusable/CardOfferCard.jsx",["198","199"],[],"import React from \"react\";\n\nconst CardOfferCard = (props) => {\n\n    //Destructure the title and icon from the props object\n    const {title, icon} = props.item\n    return(\n        <div className=\"cardoffer__card\">\n                        <div className=\"cardoffer__content\">\n                            <h4>{title}</h4>\n                            </div>\n                            <span className=\"cardoffer__icon\">\n                                <i className= {icon}></i>\n                            </span>\n                            <div className=\"cardoffer__actions\">\n                            <button className=\"cardoffer__btn\">View</button>\n                            <a href=\"#\"></a>     {/*This link doesn't have any text or content*/}\n        </div>\n                    </div>\n    );\n};\n\nexport default CardOfferCard;\n",{"ruleId":"200","severity":1,"message":"201","line":17,"column":29,"nodeType":"202","endLine":17,"endColumn":41},{"ruleId":"203","severity":1,"message":"204","line":17,"column":29,"nodeType":"202","endLine":17,"endColumn":41},{"ruleId":"205","severity":1,"message":"206","line":8,"column":8,"nodeType":"207","messageId":"208","endLine":8,"endC